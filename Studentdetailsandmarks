CREATE TABLE Student5 (
    StudentID int PRIMARY KEY,
    StudentName VARCHAR(30) NOT NULL,
    Address VARCHAR(50) NOT NULL,
    email VARCHAR(25),
    PhoneNo VARCHAR(16),
    Gender CHAR(8)
);
DESC Student5;
