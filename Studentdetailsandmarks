CREATE TABLE StudentMarks(
    RegisterNO int PRIMARY KEY,
    StudentName VARCHAR(30) NOT NULL,
    Marks int,
    Grade VARCHAR(4),
    Percentage VARCHAR(6)
)
DESC StudentMarks;
INSERT INTO StudentMarks VALUES(345,'Sasirekha',467,'A+',79);
